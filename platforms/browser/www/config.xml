<?xml version='1.0' encoding='utf-8'?>
<widget id="com.task.api" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Friday Api Task</name>
    <description>
        Aplicación para la administración de tareas de la gerencia de informatica.
    </description>
    <author email="programadores@apiqroo.com.mx" href="http://apiqroo.com.mx">
        Gerencia de Informática
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
